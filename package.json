    "gh-pages": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-libsquoosh": "^1.1.2",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
